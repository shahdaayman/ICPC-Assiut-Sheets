#include<iostream>
using namespace std;
int main() {
	int t;
	cin >> t;
	while (t--) {
		string s,s2;
		cin >> s>>s2;
		for (int i = 0;i < 50;i++) {
			if (i >= 0 && i < s.size()) {
				cout << s[i];
			}
			if (i >= 0 && i < s2.size()) {
				cout << s2[i];
			}
		}
		cout << endl;

	}
	return 0;
}
