#include<iostream>
#include<string>
using namespace std;
int main() {
	string s;
	getline(cin, s);
	int cnt[5] = {};
	
	for (int i = 0;i < s.size();i++) {
		if (s[i] == 'e' || s[i]=='E') {
			cnt[0]++;
		}
		else if (s[i] == 'g' || s[i] == 'G') {
			cnt[1]++;
		}
		else if (s[i] == 'y' || s[i] == 'Y') {
			cnt[2]++;
		}
		else if (s[i] == 'p' || s[i] == 'P') {
			cnt[3]++;
		}
		else if (s[i] == 't' || s[i] == 'T') {
			cnt[4]++;
		}
	}
	int minimum = cnt[0];
	for (int i = 0;i < 5;i++) {
		if (cnt[i] < minimum) {
			minimum = cnt[i];
		}
	}

	cout << minimum << endl;
	return 0;
}




//Notes:
//1- it's better to search for frequency array :)


