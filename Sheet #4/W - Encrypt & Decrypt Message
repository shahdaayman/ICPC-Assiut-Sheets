#include<iostream>
#include<string>
using namespace std;
int main() {
	int q;
	string s;
	string key = "PgEfTYaWGHjDAmxQqFLRpCJBownyUKZXkbvzIdshurMilNSVOtec#@_!=.+-*/";
	string original = "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789";
	cin >> q;
	cin >> s;
	for (int i = 0;i < s.size();i++) {
		for (int j = 0;j< key.size();j++) {
			if (q == 1) {
				if (s[i] == original[j]) {
					cout << key[j];
				}
			}
			else {
				if (s[i] == key[j]) {
					cout << original[j];
				}
			}
		}
	}
	return 0;
}
