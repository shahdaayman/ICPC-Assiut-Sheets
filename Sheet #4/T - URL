#include<iostream>
using namespace std;
int main() {
	string s;
	cin >> s;
	s += '&'; //to match the condition
	int start = 0, end = 0, j = 0;
	string answer[5];
	for (int i = 0;i < s.size();i++) {
		if (s[i] == '=') {
			start = i + 1;
		}
		else if (s[i] == '&') {
			end = i - 1;
			answer[j] = s.substr(start, end - start + 1);
			//=test&    //end-start+1
			//012345   //4-1+1=4 -> last letter index
			j++;
		}
	}
	string value[5] = { "username:", "pwd:", "profile:", "role:", "key:" };
	for (int i = 0;i < 5;i++) {
		cout << value[i] << " " << answer[i] << endl;
	}
	return 0;
}
