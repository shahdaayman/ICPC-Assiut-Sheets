#include<iostream>
#include<cmath>
using namespace std;
int main() {
	long long arr[100000];
	int size;
	long long sum=0;
	cin >> size;
	for (int i = 0;i < size;i++) {
		cin >> arr[i];
		sum += arr[i];
	}
	cout << abs(sum) << endl;
	return 0;
}
//====================================================================
//Another Solution:
#include<iostream>
#include<cmath>
#include<vector>
using namespace std;
int main() {
	vector<long long> v;
	int size;
	cin >> size;
	v.resize(size);
	long long sum = 0;
	for (int i = 0;i < size;i++) {
		cin >> v[i];
		sum += v[i];
	}
	cout << abs(sum) << endl;
	return 0;
}
