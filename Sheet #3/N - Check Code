#include<iostream>
#include<string>
using namespace std;
int main() {
	int a, b;
	string s;
	cin >> a >> b;
	cin >> s;
	
	bool flag = true;
	if (s[a] != '-') {
		flag = false;
	}
	for (int i = 0;i < s.size();i++) {
		if (i == a) {
			continue;
		}
		else if (s[i] == '-') {
			flag = false;
		}
	}
	if (flag) {
		cout << "Yes" << endl;
	}
	else {
		cout << "No" << endl;
	}
	return 0;

}
