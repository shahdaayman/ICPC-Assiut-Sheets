#include<iostream>
using namespace std;
int main() {
	int arr[1000];
	int size;
	cin >> size;
	for (int i = 0;i < size;i++) {
		cin >> arr[i];
	}
	for (int i = size - 1;i >= 0;i--) {
		cout << arr[i] << " ";
	}
	return 0;
}
//==================================================
//Another Solution:
#include<iostream>
#include<vector>
using namespace std;
int main() {
	vector<int> v;
	int size;
	cin >> size;
	v.resize(size);
	for (int i = 0;i < size;i++) {
		cin >> v[i];
	}
	for (int i = size-1;i >= 0;i--) {
		cout << v[i] << " ";
	}

	return 0;
}
