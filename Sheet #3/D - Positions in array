#include<iostream>
using namespace std;
int main() {
	int arr[1000];
	int size;
	cin >> size;
	for (int i = 0;i < size;i++) {
		cin >> arr[i];
		if (arr[i] <= 10) {
			cout << "A[" << i << "]" << " = " << arr[i] << endl;
		}
	}
	return 0;
}
//==================================================================
//Another Solution:
#include<iostream>
#include<vector>
using namespace std;
int main() {
	vector<int> v;
	int size;
	cin >> size;
	v.resize(size);
	for (int i = 0;i < size;i++) {
		cin >> v[i];
		if (v[i] <= 10) {
        cout << "A[" << i << "]" << " = " << v[i] << endl;

		}
	}
	return 0;
}
