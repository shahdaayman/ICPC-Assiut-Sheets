#include<iostream>
using namespace std;
int main() {
	long long arr[200];
	int size;
	bool flag = true;
	int cnt = 0;
	cin >> size;
	for (int i = 0;i < size;i++) {
		cin >> arr[i];
	}
	while(flag){
		for (int i = 0;i < size;i++) {
			if (arr[i] % 2 != 0) {
				flag = false;
			}
		}
		if (flag) {
			for (int i = 0;i < size;i++) {
				arr[i] /= 2;
			}
			cnt++;
		}
		else {
			cout << cnt << endl;
			return 0;
		}
	}
	return 0;
}
