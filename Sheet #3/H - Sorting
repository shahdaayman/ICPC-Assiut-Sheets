1- Array- Bubble Sort:
#include <iostream>
using namespace std;
int main() {
    int arr[1000];
    int size;
    cin >> size;
    for (int i = 0;i < size;i++) {
        cin >> arr[i];
    }
    for (int i = 0;i < size-1;i++) {
        for (int j = 0;j < size - 1;j++) {
            if (arr[j + 1] < arr[j]) {
                swap(arr[j], arr[j + 1]);
            }
        }
    }
    for (int i = 0;i < size;i++) {
        cout << arr[i] << " ";
    }
    return 0;
}
//=========================================================
2- Vector-Bubble Sort:
#include <iostream>
#include<vector>
using namespace std;
int main() {
    vector<int>v;
    int size;
    cin >> size;
    v.resize(size);
    for (int i = 0;i < size;i++) {
        cin >> v[i];
    }
    for (int i = 0;i < size-1;i++) {
        for (int j = 0;j < size - 1;j++) {
            if (v[j + 1] < v[j]) {
                swap(v[j], v[j + 1]);
            }
        }
    }
    for (int i = 0;i < size;i++) {
        cout << v[i] << " ";
    }
    return 0;
}
//=========================================================


