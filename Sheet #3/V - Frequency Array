#include<iostream>
using namespace std;
int main() {
	int n, m;
	int cnt = 0;
	cin >> n>> m;
	int arr[100000];
	int temp[100000] = {};
	for (int i = 0;i < n;i++) {
		cin >> arr[i];
	}
	for (int i = 0;i < n;i++) {
		temp[arr[i]]++;
	}
	for (int i = 1;i < m+1;i++) {
		cout << temp[i] << endl;
	}
	return 0;
}
