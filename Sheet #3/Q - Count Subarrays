#include<iostream>
using namespace std;
int main() {
	int t;
	cin >> t;
	while (t--) {
		int arr[1000];
		int size;
		int cnt = 0;
		cin >> size;
		cnt = size;
		for (int i = 0;i < size;i++) {
			cin >> arr[i];
		}
		for (int i = 0;i < size;i++) {
			for (int j = i + 1;j < size;j++) {
				if (arr[j] > arr[j-1]) {
					cnt++;
				}
				else {
					break;
				}
			}
		}
		cout<<cnt<<endl;
	}
	return 0;
}
