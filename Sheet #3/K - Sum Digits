#include <iostream>
using namespace std;
int main() {
    string s;
    int size;
    int sum = 0;
    cin >> size;
    cin >> s;
    for (int i = 0;i < s.size();i++) {
        sum += s[i] - '0';
    }
    cout << sum << endl;
    return 0;
}
//===========================================================
//Another Solution:
#include <iostream>
using namespace std;
int main() {
    char arr[1000000];
    int size;
    long long sum = 0;
    cin >> size;
    for (int i = 0;i < size;i++) {
        cin >> arr[i];
    }
    for (int i = 0;i < size;i++) {
        sum += arr[i] - '0';
    }
    cout << sum << endl;
    return 0;
}
//===========================================================
//Another Solution:
#include<iostream>
#include<vector>
#include<algorithm>
using namespace std;
int main() {
	vector<char> v;
	int size;
	long long sum = 0;
	cin >> size;
	v.resize(size);
	for (int i = 0;i < size;i++) {
		cin >> v[i];
	}
	for (int i = 0;i < size;i++) {
		sum += v[i] - '0';
	}
	cout << sum << endl;
}
