#include<iostream>
using namespace std;
int main() {
	int n;
	int cnt = 0;
	cin >> n;
	int arr1[1000];
	int arr2[1000];
	for (int i = 0;i < n;i++) {
		cin >> arr1[i];
	}

	for (int i = 0;i < n;i++) {
		cin >> arr2[i];
	}

	for (int i = 0;i < n;i++) {
		for (int j = 0;j < n;j++) {
			if (arr1[i] == arr2[j]) {
				cnt++;
				arr1[i] = -9999;
				arr2[j] = -9875;
			}
		}
	}
	if (cnt == n) {
		cout << "yes" << endl;
	}
	else {
		cout << "no" << endl;
	}
	return 0;
}
