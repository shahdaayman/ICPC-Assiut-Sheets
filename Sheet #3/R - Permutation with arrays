#include<iostream>
using namespace std;
int main() {
	int n;
	int cnt = 0;
	cin >> n;
	int arr1[1000];
	int arr2[1000];
	for (int i = 0;i < n;i++) {
		cin >> arr1[i];
	}

	for (int i = 0;i < n;i++) {
		cin >> arr2[i];
	}

	for (int i = 0;i < n;i++) {
		for (int j = 0;j < n;j++) {
			if (arr1[i] == arr2[j]) {
				cnt++;
				arr1[i] = -9999;
				arr2[j] = -9875;
			}
		}
	}
	if (cnt == n) {
		cout << "yes" << endl;
	}
	else {
		cout << "no" << endl;
	}
	return 0;
}

//===========================================================
//Another Solution:
#include<iostream>
#include<algorithm>
using namespace std;
int main() {
	int n;
	int arr1[1000], arr2[1000];
	bool flag = true;
	cin >> n;
	for (int i = 0;i < n;i++) {
		cin >> arr1[i];
	}
	for (int i = 0;i < n;i++) {
		cin >> arr2[i];
	}
	sort(arr1, arr1 + n);
	sort(arr2, arr2 + n);
	
	for (int i = 0;i < n;i++) {
		if (arr1[i] != arr2[i]) {
			flag = false;
			break;
		 }
	}
	
	if (flag) {
		cout << "yes" << endl;
	}
	else {
		cout << "no" << endl;
	}
	return 0;
}
