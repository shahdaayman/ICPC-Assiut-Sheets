#include<iostream>
using namespace std;
int main() {
	int n, m;
	cin >> n >> m;
	long long arr[n], arr2[m];
	for (int i = 0;i < n;i++) {
		cin >> arr[i];
	}
	for (int i = 0;i < m;i++) {
		cin >> arr2[i];
	}

	int cnt = 0;
	for (int i = 0;i < n;i++) {
		if (arr2[cnt] == arr[i]) {
			cnt++;
		}
	}

	if (cnt >= m) {
		cout << "YES" << endl;
	}
	else {
		cout << "NO" << endl;
	}
	return 0;
}
