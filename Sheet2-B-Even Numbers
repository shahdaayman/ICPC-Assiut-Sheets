#include<iostream>
using namespace std;
int main() {
	int N;
	cin >> N;
	if (N == 1) {
		cout << -1 << endl;
	}
	else {
		for (int i = 1;i <= N;i++) {
			if (i % 2 == 0) {
				cout << i << endl;
			}
		}
	}
	return 0;
}

//Note:
//The only case that there is no even numbers is if N=1 
//================================================
//Another solution:
#include<iostream>
using namespace std;
int main() {
	int N;
	cin >> N;
	if (N == 1) {
		cout << -1 << endl;
	}
	else {
		for (int i = 2;i <= N;i+=2) {

			cout << i << endl;
		}
	}
	return 0;
}
