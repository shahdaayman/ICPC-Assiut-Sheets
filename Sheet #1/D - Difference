//C++:
#include<iostream>
using namespace std;
int main() {
	long long A, B, C, D;
	cin >> A >> B >> C >> D;
	long long X = (A * B) - (C * D);
	cout << "Difference = " << X << endl;
	return 0;
}
//============================================================
//Python:
data= input().split()
A= int(data[0])
B= int(data[1])
C=int(data[2])
D=int(data[3])
total=(A*B)-(C*D)
print(f"Difference = {total}")
