//C++:
#include<iostream>
using namespace std;
int main() {
	long long x, y;
	cin >> x >> y;
	cout << x << " + " << y << " = " << x + y << endl;
	cout << x << " * " << y << " = " << x * y << endl;
	cout << x << " - " << y << " = " << x - y << endl;
	return 0;
}
//======================================================================
//Python:
data = input().split()

num = int(data[0])
num2 = int(data[1])

print(f"{num} + {num2} = {num+num2}")
print(f"{num} * {num2} = {num*num2}")
print(f"{num} - {num2} = {num-num2}")
