#include <iostream>
#include<cmath>
using namespace std;

void isPrime(int n) {
    if (n < 2) {
        cout << "NO" << endl;
        return;
    }

    int cnt = 0;
    for (int i = 2; i <= sqrt(n); i++) {
        if (n % i == 0) {
            cnt++;
        }
    }

    if (cnt == 0) {
        cout << "YES" << endl;
    }
    else {
        cout << "NO" << endl;
    }
}


int main() {
    int t, n;
    cin >> t;

    while (t--) {
        cin >> n;
        isPrime(n);
    }
    return 0;
}
