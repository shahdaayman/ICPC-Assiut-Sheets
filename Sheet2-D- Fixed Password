#include<iostream>
using namespace std;
int main() {
	int num;
	cin >> num;
	while (num != 1999) {
		cin >> num;
		cout << "Wrong" << endl;
	}
	cout << "Correct" << endl;
	return 0;
}

//==============================================
//Another solution:
#include<iostream>
using namespace std;
int main() {
	int num;
	while (true) {
		cin >> num;
		if (num == 1999) {
			cout << "Correct" << endl;
			break;
		}
		else {
			cout << "Wrong" << endl;
		}
	}
	return 0;
}
