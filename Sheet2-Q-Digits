#include<iostream>
#include<string>
#include<algorithm>
using namespace std;
int main() {
    string str;
    int temp;
    cin >> temp;

    for (int i = 1;i <= temp;i++) { 
        cin >> str;   
        reverse(str.begin(), str.end());
        for (int j = 0;j < str.size();j++) {
            cout << str[j] << " ";
        }
        cout << endl;
    }
    return 0;
}

//======================================================================
#include<iostream>
using namespace std;
int main() {
    int t;
    int n;
    cin >> t;
    while (t--) {
        cin >> n;
        if (n == 0) {
            cout << 0;
        }
        else {
            while (n != 0) {
               int temp = n % 10;
                n /= 10;
                cout << temp << " ";
            }
        }
        cout<<endl;
    }
    return 0;
}



